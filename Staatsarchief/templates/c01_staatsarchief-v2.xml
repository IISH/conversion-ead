<c01 level="file">
    <did>
        <container type="box"></container>
        <unitid>{callno}</unitid>
        <unittitle>{title}.
            <unitdate>{period}.</unitdate>
        </unittitle>
        <physdesc>
            <genreform>{format}</genreform>
        </physdesc>
    </did>
    <scopecontent>
        <p>{notes}</p>
    </scopecontent>
    <odd type="subject">
        <p>Trefwoorden: {subject}</p>
    </odd>
    <odd type="geographical">
        <p>Geografisch onderwerp: {geographical}</p>
    </odd>
    <odd type="person">
        <p>Personen: {person}</p>
    </odd>
    <odd type="organization">
        <p>Organisaties: {organization}</p>
    </odd>
    <odd type="notes">
        <p>Opmerkingen: {notes}</p>
    </odd>
</c01>